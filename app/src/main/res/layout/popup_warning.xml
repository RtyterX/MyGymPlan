<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/pop_Warning"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="5dp" >

    <View
        android:id="@+id/viewTeste3"
        android:layout_width="318dp"
        android:layout_height="134dp"
        android:background="#F1F1FF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/WarningMessage"
        android:layout_width="213dp"
        android:layout_height="50dp"
        android:layout_marginTop="28dp"
        android:text="Warning Message"
        android:textAlignment="center"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/viewTeste3"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="@+id/viewTeste3"
        app:layout_constraintTop_toTopOf="@+id/viewTeste3" />

    <Button
        android:id="@+id/CloseWarningButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="64dp"
        android:text="Close"
        app:layout_constraintStart_toStartOf="@+id/viewTeste3"
        app:layout_constraintTop_toBottomOf="@+id/WarningMessage" />

    <Button
        android:id="@+id/ConfirmWarningButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="64dp"
        android:text="Confirm"
        app:layout_constraintEnd_toEndOf="@+id/viewTeste3"
        app:layout_constraintTop_toBottomOf="@+id/WarningMessage" />

</androidx.constraintlayout.widget.ConstraintLayout>